public class Fun009 {
	public String main(){
        return String.valueOf(getValue(2));
	}
	
	public int getValue(int i){
		int result = 0;
        switch (i) {
        case 1:
            result = result + i;
        case 2:
            result = result + i * 2;
        case 3:
            result = result + i * 3;
        }
        return result;
	}
}